{
  "manifest_version": 2,
  "name": "chrome-jwt-view",
  "version": "1.0.0",
  "description": "Google Chrome extension to view, parse and validate JSON Web Tokens",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
